{
  "name": "boodschappen-route",
  "version": "0.1.0",
  "description": "Simple express server with sequelize, pg and jwts",
  "main": "index.js",
  "scripts": {
    "start": "node .",
    "dev": "env DELAY=1000 npx nodemon .",
    "initdev": "npm run migrate && npm run seed",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "resetDB": "npx sequelize-cli db:migrate:undo:all && npm run initdev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "template",
    "express",
    "jwt",
    "sequelize"
  ],
  "author": "Fernando Fernandes Simao",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "instal": "^0.0.1-security",
    "install": "^0.13.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.1",
    "sequelize": "^6.8.0",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "axios": "^0.19.2",
    "nodemon": "^2.0.13"
  }
}
